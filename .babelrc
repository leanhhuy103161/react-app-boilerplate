{
  "presets": [
    "react-app",
    "@babel/preset-env"
  ],
  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "@assets": "./src/assets",
        "@img": "./src/assets/img",
        "@components": "./src/components",
        "@containers": "./src/containers",
        "@layout": "./src/layout",
        "@locales": "./src/locales",
        "@modules": "./src/modules",
        "@routes": "./src/routes",
        "@services": "./src/services",
        "@stores": "./src/stores",
        "@utils": "./src/utils"
      }
    }]
  ]
}
